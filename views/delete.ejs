<%- include('standard/header.ejs') %>
<div class="blog-contain">
    <h1>Do you want to delete this Blog</h1>
<form action="/delete" method="post" class="blog-form">
    <div>
    <input type="hidden" id="blogid" name="blogid" value="<%= data %>">
    </div>
    <div>   
        <input type="submit" value="yes" name="yes" class="blog-submit" >
    </div>
    <br><br>
    <div>   
        <input type="submit" value="no" name="no" class="blog-submit" >
    </div>
</form>
</div>
